        float beta = k.x/d.x,
            alpha = d.y*k.x/d.x-k.y;
        
        n += step(.0, beta)*step(beta, 1.)*step(0., alpha);
        ret = min(ret, dline2(x, ptsi, ptsip1));
    }
    
    ret = mix(ret, -ret, mod(n, 2.));
    
    ret /= .7;
    return ret;
}

