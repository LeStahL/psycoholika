    //     x.xy -= vec2(.4,-.2);

    //     sdf = vec3(1., -dbox3(x, 2.*c.xxx+1.*c.xyy), 0.);

    //     RR = rot3(.4*iTime*vec3(1.1,1.3,1.7));
    //     x = RR * x;

    //     float size = .2,
